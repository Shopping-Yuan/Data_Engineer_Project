{
  "name": "Python Development Container",
  "dockerFile": "crawl_dockerfile",
  "context": ".",
  "settings": {
    "python.pythonPath": "/usr/local/bin/python"
  },
  "extensions": [
    "ms-python.python",
    "ms-toolsai.jupyter"
  ],
  "mounts": [
    "source=${localWorkspaceFolder}/DATA_ENGINEER_PROJECT,target=/workspace/data_project,type=bind"
  ],
  "remoteUser": "vscode"
}